{
  "buildCommand": "npm run build",
  "outputDirectory": "frontend/dist",
  "installCommand": "npm run install:all",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.ts"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/crawl",
      "schedule": "0 9 * * *"
    }
  ]
}